<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	
	<bean id="houseLandDao" class="com.nagarpalika.dao.daoimpl.HouselandDaoImpl">
   		<property name="jdbcTemplate" ref="jdbcTemplate" />
   </bean>
	
	<bean id="jdbcTemplate" class="org.springframework.jdbc.core.JdbcTemplate">
		<property name="dataSource" ref="dataSource" />
	</bean>
	<bean id="userDao" class="com.nagarpalika.dao.daoimpl.UserDaoImpl">
		<property name="jdbcTemplate" ref="jdbcTemplate" />
	</bean>
	<bean id="roleDao" class="com.nagarpalika.dao.daoimpl.RoleDaoImpl">
		<property name="jdbcTemplate" ref="jdbcTemplate" />
	</bean>
	<bean id="branchDao" class="com.nagarpalika.dao.daoimpl.BranchDaoImpl">
		<property name="jdbcTemplate" ref="jdbcTemplate" />
	</bean>
	<bean id="operationDao" class="com.nagarpalika.dao.daoimpl.OperationDaoImpl">
		<property name="jdbcTemplate" ref="jdbcTemplate" />
	</bean>
	<bean id="educationDetailDao" class="com.nagarpalika.dao.daoimpl.EducationDetailDaoImpl">
		<property name="jdbcTemplate" ref="jdbcTemplate" />
	</bean>
	<bean id="documentDetailDao" class="com.nagarpalika.dao.daoimpl.DocumentTypeDaoImpl">
		<property name="jdbcTemplate" ref="jdbcTemplate" />
	</bean>
	<bean id="occupationDao" class="com.nagarpalika.dao.daoimpl.OccupationDaoImpl">
		<property name="jdbcTemplate" ref="jdbcTemplate" />
	</bean>
	<!-- Root Context: defines shared resources visible to all other web components -->
	<bean id="dataSource"
		class="org.springframework.jdbc.datasource.DriverManagerDataSource">
		<property name="driverClassName" value="com.mysql.jdbc.Driver" />
		<property name="url" value="jdbc:mysql://localhost:3306/mmis_01" />
		<property name="username" value="root" />
		<property name="password" value="" />
	</bean>
		
</beans>
